---
permalink: /tags/
layout: layouts/home.njk
---
<h1>Tags</h1>

<ul class="page-tags-list">
{% for tag in collections.all | getAllTags | filterTagList %}
	{% set tagUrl %}/tags/{{ tag | slugify }}/{% endset %}
	<li class="page-tags-list-item"><a href="{{ tagUrl }}" class="post-tag">{{ tag }}</a></li>
{% endfor %}
</ul>
